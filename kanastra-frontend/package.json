{
  "name": "kanastra-challenge-boilerplate",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently --kill-others \"bun run tailwindcss --watch\" \"vite\"",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "tailwindcss": "tailwindcss -i ./src/global.css -o ./dist/index.css",
    "build": "tsc && vite build && bun run tailwindcss",
    "preview": "vite preview",
    "dev:node": "concurrently \"npx tailwindcss -i ./src/global.css -o ./dist/index.css --watch\" \"vite\"",
    "build:node": "tsc && vite build && npm run tailwindcss",
    "test": "npx jest"
  },
  "dependencies": {
    "autoprefixer": "^10.4.16",
    "axios": "^1.7.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "concurrently": "^8.2.2",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.2.1",
    "react-paginate": "^8.2.0",
    "react-router-dom": "^6.23.1",
    "react-toastify": "^10.0.5",
    "sort-by": "^1.2.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.8.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.0.0",
    "axios-mock-adapter": "^1.22.0",
    "cypress": "^13.10.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "tailwindcss": "^3.3.3",
    "ts-jest": "^29.1.3",
    "typescript": "^5.0.2",
    "vite": "^4.3.2"
  }
}
